{
	"manifest_version": 2,
	"name": "CACKey",
	"version": "@PACKAGE_VERSION@.2",

	"icons": {
		"128": "icon.png"
	},

	"app": {
		"background": {
			"scripts": [
				"google-pcsc.js",
				"cackey.js"
			],
			"persistent": false
		}
	},

	"permissions": [
		"certificateProvider",
		"alwaysOnTopWindows"
	]
}
